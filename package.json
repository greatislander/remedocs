{
  "name": "remedocs",
  "version": "1.0.0",
  "description": "11ty docs for remedy",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build-site": "npx @11ty/eleventy",
    "build-sass": "sass sass/style.scss src/css/style.css",
    "build": "yarn build-sass && yarn build-site",
    "serve": "yarn build-sass && npx @11ty/eleventy --serve",
    "watch": "sass --watch sass/style.scss src/css/style.css"
  },
  "devDependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "cssremedy": "^0.1.0-beta.1",
    "doxray": "^0.8.1",
    "eleventy": "^0.2.0",
    "markdown-it": "^10.0.0",
    "markdown-it-prism": "^2.0.3",
    "sass": "^1.22.12"
  }
}
